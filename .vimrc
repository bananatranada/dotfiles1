syntax on
set number
set autoindent
set smartindent
set lazyredraw
set visualbell
set backspace=indent,eol,start
set timeoutlen=500
set hlsearch
set hidden
set ignorecase smartcase
set tabstop=2
set shiftwidth=2
set expandtab smarttab
set encoding=utf-8
set colorcolumn=80

vnoremap > >gv
vnoremap < <gv
